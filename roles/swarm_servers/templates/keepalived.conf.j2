vrrp_instance VI_1 {
        state {{ vrrp_state }}
        interface eth0
        virtual_router_id 51
        priority {{ vrrp_priority }}
        advert_int 1
        authentication {
              auth_type PASS
              auth_pass {{ hostvars[groups['masters'][0]]['vrrp_password']['stdout'] }}
        }
        virtual_ipaddress {
              {{ swarm_vipip }}
        }
}
